{
  "coc.preferences.formatOnSaveFiletypes": [
    "Markdown",
    "css",
    "graphql",
    "jasmine.javascript",
    "javascript",
    "javascriptreact",
    "json",
    "typescript",
    "typescriptreact"
  ],
  "coc.preferences.rootPatterns": [".vim", ".git", ".projections.json"],
  "eslint.autoFixOnSave": true,
  "eslint.probe": [
    "jasmine.javascript",
    "javascript",
    "javascriptreact",
    "json",
    "typescript",
    "typescriptreact"
  ],
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "sql.lintOnChange": true,
  "sql.lintOnSave": true,
  "suggest.floatEnable": true,
  "tsserver.enable": true,
  "tsserver.enableJavascript": true
}
